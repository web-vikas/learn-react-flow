generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model WorkFlow {
  id          String  @id @default(cuid()) @map("_id")
  name        String
  user_id     String
  description String?

  definitions String
  status      String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([name, user_id])
}
